{
  "name": "vue-d3-cloud",
  "description": "a word cloud component using d3-cloud",
  "keywords": [
    "word-cloud",
    "vue",
    "vuejs",
    "d3"
  ],
  "author": "makeupsomethinglater",
  "license": "MIT",
  "homepage": "https://github.com/makeupsomething/vue-d3-cloud",
  "bugs": {
    "url": "https://github.com/makeupsomething/vue-d3-cloud/issues"
  },
  "version": "0.1.6",
  "private": false,
  "main": "./dist/vue-d3-cloud.common.js",
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --name vue-d3-cloud ./src/components/index.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "d3": "^5.7.0",
    "d3-cloud": "^1.2.5",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
